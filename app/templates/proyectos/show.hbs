<h1 class="page-header">
  <span class="pull-right text-muted">{{fa-icon "eye" fixedWidth=true}}</span>
  {{#link-to "proyectos.index" tagName="a" }}{{fa-icon "arrow-left" fixedWidth=true}}{{/link-to}}
  Proyecto
</h1>

<div class="form-horizontal">

  <div class="form-group">
    <label class="control-label col-md-4">Nombre</label>
    <div class="col-md-8">
      <p class="form-control-static">
        {{model.nombre}}
      </p>
    </div>
  </div>

  <div class="form-group">
    <label class="control-label col-md-4">Descripcion</label>
    <div class="col-md-8">
      <p class="form-control-static">
        {{model.descripcion}}
      </p>
    </div>
  </div>

  <div class="form-group">
    <label class="control-label col-md-4">Fecha inicio</label>
    <div class="col-md-8">
      <p class="form-control-static">
        {{model.fechainicio}}
      </p>
    </div>
  </div>

  <div class="form-group">
    <label class="control-label col-md-4">Fecha fin</label>
    <div class="col-md-8">
      <p class="form-control-static">
        {{model.fechafin}}
      </p>
    </div>
  </div>

</div>
